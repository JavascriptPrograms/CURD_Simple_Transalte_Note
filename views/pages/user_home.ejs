<div class="container-fluid mt-3">
    <% if(users){%>
        <div class="row">
            <div class="col-sm-8">
                <% users.map((items)=>{%>
                    <div class="card mb-3">
                        <div class="card-header bg-primary text-white"><%= items.chapter_no %></div>
                        <div class="card-body"><%= items.structure_no %></div>
                        <div class="card-footer"><%= items.translate %></div>
                    </div>
                <%})%>
            </div>
            <div class="col-sm-4">
                <h2>All Chapter</h2>
                <div class="card">
                    <% users.map((items)=>{%>
                        <a href="#"><div class="card-header bg-primary text-white"><%= items.chapter_no %></div></a>
                        <div class="card-body"><%= items.structure_no %></div>
                    <%})%>
                </div>
            </div>
        </div>
        
    <% } else { %>
        <p>No data found.</p>
    <% } %>
</div>
<ul class="pagination pagination-sm justify-content-center">
    <% if (currentPage > 1) { %>
        <li class="page-item"><a class="page-link" href="/?page=<%= currentPage - 1 %>">Previous</a></li>
    <% } %>
    <% for (let i = 1; i <= totalPages; i++) { %>
    <li class="page-item"><a class="page-link" href="/?page=<%= i %>" <%= currentPage === i  %>><%= i %></a></li>
    <% } %>
    <% if (currentPage < totalPages) { %>
        <li class="page-item"><a class="page-link" href="/?page=<%= currentPage + 1 %>">Next</a></li>
    <% } %>
  </ul>
<div>

